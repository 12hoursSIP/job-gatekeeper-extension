{
  "manifest_version": 3,
  "name": "Job Gatekeeper",
  "version": "1.0",
  "description": "Analyzes job descriptions and generates SJT questions.",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Job Gatekeeper"
  }
}